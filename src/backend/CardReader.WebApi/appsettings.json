{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Error"
      }
    },
    "Enrich": [
      "FromLogContext"
    ]
  },
  "ConnectionStrings": {
    "Database": "<DB_CONNECTION_STRING>"
  },
  "AllowedHosts": "*"
}